<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.taxi.model">
	<class name="PassInfo"  table="i_Pass" >
		<id name="ID">
				<generator class="foreign">
				<param name="property">Tuser</param>
			</generator>
		</id>
		<property name="Name" not-null="true"/>
		<property name="Age"/>
		<property name="Score"/>
		<property name="Sex"/>
		<property name="Address"/>
		<property name="Email"/>
		<one-to-one name="Tuser" constrained="true"/>
	
	</class>
</hibernate-mapping>