<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.taxi.model">
	<class name="OrderHistory"  table="i_Order" >
		<id name="Oid">
			<generator class="assigned"/>
		</id>
		<property name="Type"/>
		<property name="Odate"/>
		<property name="Did"/>
		<property name="Pid"/>
		<property name="Start"/>
		<property name="Finish"/>
		<property name="Status"/>
		<property name="Tip"/>
		
	    <!--   <many-to-one name="Did" column="Did"/>
	     <many-to-one name="Pid" column="Pid"/>
	     -->
	</class>
</hibernate-mapping>




<!--  此配置 Did Pid 为外键 多对一指向DiviInfo PassInfo  -->