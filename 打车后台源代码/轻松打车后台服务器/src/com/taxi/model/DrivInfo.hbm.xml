<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.taxi.model.DrivInfo" table="i_Driv">
		<id name="ID">
			<generator class="foreign">
				<param name="property">Tuser</param>
			</generator>
		</id>
		
		<property name="Name" not-null="true"/>
		<property name="Age"/>
		<property name="Score"/>
		<property name="Sex"/>
		<property name="Email"/>
		<property name="Plate"/>
		<property name="License"/>
		<property name="Model"/>
		<one-to-one name="Tuser" constrained="true"/>
	</class>
</hibernate-mapping>